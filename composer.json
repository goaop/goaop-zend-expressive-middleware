{
  "name": "goaop/goaop-zend-expressive-middleware",
  "type": "library",
  "description": "An Zend Expressive middleware for loading goaop aspects.",
  "keywords": [
    "zend",
    "expressive",
    "middleware",
    "goaop"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Reinfi"
    }
  ],
  "require": {
    "php": ">=7.1",
    "goaop/framework": "^2.1",
    "zendframework/zend-servicemanager": "^3.0",
    "psr/http-server-middleware": "^1.0"
  },
  "bin": [
    "bin/goaop-expressive-warmup"
  ],
  "autoload": {
    "psr-4": {
      "Go\\Zend\\Expressive\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Go\\Zend\\Expressive\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/console": "^3.3",
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-expressive": "^3.0",
    "zendframework/zend-config-aggregator": "^1.0",
    "zendframework/zend-diactoros": "^1.7 || ^2.0"
  },
  "extra": {
    "zf": {
      "component": "Go\\Zend\\Expressive",
      "config-provider": "Go\\Zend\\Expressive\\ConfigProvider"
    }
  }
}
